{
  "name": "cypress_automation",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "node_modules/.bin/cypress run",
    "headTest": "npm test run -- -- headed",
    "chromeTest": "npm run test -- -- browser chrome",
    "chromeTestParallel": "npm run test -- -- browser chrome --parallel",
    "recordDashBoardTest": "npm run test -- --record --key d2ac379d-93e3-486f-b5a9-32ea2e44f965 --reporter mochawesome",
    "GreenKartTest": "npm run test -- --spec \"cypress/integration/GreenKart/*\""
  },
  "author": "Mehmet Altuntas",
  "license": "ISC",
  "devDependencies": {
    "cypress": "^12.9.0",
    "mocha": "^10.1.0",
    "multiple-cucumber-html-reporter": "^3.0.1",
    "cypress-iframe": "^1.0.1"
  },
  
   
  "cypress-cucumber-preprocessor": {
    "json": {
      "enabled": true,
      "output": "cypress/cucumberReports/results.json"
    }
  },

  "dependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^15.1.5",
    "@cypress/browserify-preprocessor": "latest",
    "init": "^0.1.2"
  },


  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mehmet-Altuntas1986/CypressAutomation.git"
  },

  "bugs": {
    "url": "https://github.com/Mehmet-Altuntas1986/CypressAutomation/issues"
  },

  "homepage": "https://github.com/Mehmet-Altuntas1986/CypressAutomation#readme",

  "description": "Cypress Automation"






}
